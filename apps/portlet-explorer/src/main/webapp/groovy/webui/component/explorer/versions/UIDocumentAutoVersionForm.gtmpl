<div class="PopupContent popupContent">
  <div>Are you sure you want to</div>
  <div class="uiActionBorder">
    <%
    int tabIndex = 1;
    for(action in uicomponent.getActions()) {
    %>
    <button type="button" onclick="<%=uicomponent.event(action)%>" tabindex=<%=tabIndex++%> class="btn $action" href="javascript:void(0);">
      <%=_ctx.appRes("UIConfirmMessage.label." + action)%></button>
    <%}%>
  </div>
</div>
