<div class="uiActivateVersion">
	<ul class="singleMessage popupMessage resizable">
		<li><span class="confirmationIcon"><%=_ctx.appRes(uicomponent.getId() + ".label.note")%> <%=_ctx.appRes(uicomponent.getId() + ".label.question")%></span></li>
