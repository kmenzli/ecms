	<div class="uiAction uiActionBorder">
		  <button type="button" onclick="<%=uicomponent.event("EnableVersion")%>" class="btn"><%= _ctx.appRes("UIActivateVersion.label.button-active") %></button>
		  <button type="button" onclick="<%=uicomponent.event("Cancel")%>" class="btn"><%= _ctx.appRes("UIActivateVersion.label.button-cancel") %></button>
	</div>  
</div>